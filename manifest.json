{
  "name": "Krithon",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "sidePanel", 
    "storage", 
    "tabs",
    "cookies",
    "nativeMessaging"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "key": "",
  "host_permissions": ["<all_urls>"]
}